{
  "name": "ngantri",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f postgres",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "docker:restart": "npm run docker:down && npm run docker:up",
    "db:generate": "npx cross-env DATABASE_URL=postgres://postgres:postgres@localhost:5433/ngantri drizzle-kit generate",
    "db:migrate": "npx cross-env DATABASE_URL=postgres://postgres:postgres@localhost:5433/ngantri drizzle-kit migrate",
    "db:studio": "npx cross-env DATABASE_URL=postgres://postgres:postgres@localhost:5433/ngantri drizzle-kit studio",
    "db:seed": "npx cross-env DATABASE_URL=postgres://postgres:postgres@localhost:5433/ngantri tsx scripts/seed.ts",
    "db:reset": "npm run docker:clean && npm run docker:up && npm run db:wait && npm run db:migrate && npm run db:seed",
    "db:wait": "npx wait-on tcp:localhost:5433",
    "setup": "npm install && npm run docker:up && npm run db:wait && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-table": "^8.21.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.6",
    "lucide-react": "^0.544.0",
    "multer": "^2.0.2",
    "next": "^15.5.7",
    "postgres": "^3.4.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "recharts": "^3.5.1",
    "sharp": "^0.34.4",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.19",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-test-api-route-handler": "^5.0.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "wait-on": "^9.0.1"
  },
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d"
}
